<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.CameraSettings" parent="@*com.google.android.GoogleCamera:style/Theme.GoogleMaterial.Dark.NoActionBar">
        <item name="android:textColorPrimary">@*com.google.android.GoogleCamera:color/google_dark_blue500</item>
        <item name="android:textColorSecondary">#b3FFFFFF</item>
        <item name="android:windowBackground">@*android:color/background_material_dark</item>
        <item name="android:preferenceCategoryStyle">@*com.google.android.GoogleCamera:style/PreferenceCategoryStyle</item>
        <item name="android:preferenceStyle">@*com.google.android.GoogleCamera:style/PreferenceStyle</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:tint">@android:color/white</item>
        <item name="android:divider">#0dFFFFFF</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:alertDialogTheme">@*com.google.android.GoogleCamera:style/CameraListPreferenceStyle</item>
        <item name="android:colorControlActivated">@*com.google.android.GoogleCamera:color/settings_accent_color</item>
        <item name="android:navigationBarColor">@*android:color/background_material_dark</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="*com.google.android.GoogleCamera:colorPrimary">@*android:color/background_material_dark</item>
    </style>
</resources>