<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:whatsapp="http://schemas.android.com/apk/com.whatsapp">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="@*com.whatsapp:dimen/conversation_share_contact_width"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_gravity="left"
            android:orientation="horizontal"
            android:id="@*com.whatsapp:id/name_in_group"
            android:paddingLeft="8.0dip"
            android:paddingTop="3.0dip"
            android:paddingRight="8.0dip"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <com.whatsapp.TextEmojiLabel
                android:textSize="@*com.whatsapp:dimen/conversation_participant_name_tv"
                android:textColor="#ffffffff"
                android:id="@*com.whatsapp:id/name_in_group_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:lines="1"
                android:singleLine="true" />

            <com.whatsapp.TextEmojiLabel
                android:textSize="@*com.whatsapp:dimen/conversation_participant_name_tv"
                android:textColor="#b3FFFFFF"
                android:id="@*com.whatsapp:id/pushname_in_group_tv"
                android:paddingLeft="12.0sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:lines="1"
                android:singleLine="true" />
        </LinearLayout>

        <FrameLayout
            android:id="@*com.whatsapp:id/quoted_message_holder"
            android:visibility="gone"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            style="@*com.whatsapp:style/QuotedFrame" />

        <LinearLayout
            android:orientation="horizontal"
            android:id="@*com.whatsapp:id/contact_card"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="8.0dip">

                <com.whatsapp.ThumbnailButton
                    android:gravity="center|left"
                    android:layout_gravity="center|left"
                    android:id="@*com.whatsapp:id/picture3"
                    android:layout_width="@*com.whatsapp:dimen/multiple_contact_message_photo_size"
                    android:layout_height="@*com.whatsapp:dimen/multiple_contact_message_photo_size"
                    android:layout_marginLeft="16.0dip"
                    android:scaleType="fitCenter"
                    whatsapp:tbtnBorderColor="@*com.whatsapp:color/white"
                    whatsapp:tbtnBorderSize="1.0dip"
                    whatsapp:tbtnForegroundOnly="false"
                    whatsapp:tbtnRadius="-1.0dip" />

                <com.whatsapp.ThumbnailButton
                    android:gravity="center|left"
                    android:layout_gravity="center|left"
                    android:id="@*com.whatsapp:id/picture2"
                    android:layout_width="@*com.whatsapp:dimen/multiple_contact_message_photo_size"
                    android:layout_height="@*com.whatsapp:dimen/multiple_contact_message_photo_size"
                    android:layout_marginLeft="-43.0dip"
                    android:scaleType="fitCenter"
                    whatsapp:tbtnBorderColor="@*com.whatsapp:color/white"
                    whatsapp:tbtnBorderSize="1.0dip"
                    whatsapp:tbtnForegroundOnly="false"
                    whatsapp:tbtnRadius="-1.0dip" />

                <com.whatsapp.ThumbnailButton
                    android:gravity="center|left"
                    android:layout_gravity="center|left"
                    android:id="@*com.whatsapp:id/picture"
                    android:layout_width="@*com.whatsapp:dimen/multiple_contact_message_photo_size"
                    android:layout_height="@*com.whatsapp:dimen/multiple_contact_message_photo_size"
                    android:layout_marginLeft="-44.0dip"
                    android:scaleType="fitCenter"
                    whatsapp:tbtnBorderColor="@*com.whatsapp:color/white"
                    whatsapp:tbtnBorderSize="1.0dip"
                    whatsapp:tbtnForegroundOnly="false"
                    whatsapp:tbtnRadius="-1.0dip" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4.0dip"
                android:minHeight="52.0dip">

                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="0.0dip"
                    android:layout_weight="1.0">

                    <com.whatsapp.TextEmojiLabel
                        android:textColor="@*com.whatsapp:drawable/flat_button_link_color_incoming"
                        android:ellipsize="end"
                        android:layout_gravity="center|left"
                        android:id="@*com.whatsapp:id/vcard_text"
                        android:paddingRight="8.0dip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxLines="3"
                        style="@*com.whatsapp:style/ConversationSharedContact" />
                </LinearLayout>

                <LinearLayout
                    android:layout_gravity="bottom|center|right"
                    android:orientation="horizontal"
                    android:id="@*com.whatsapp:id/date_wrapper"
                    android:paddingRight="6.0dip"
                    android:paddingBottom="3.0dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3.0dip">

                    <TextView
                        android:id="@*com.whatsapp:id/date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@*com.whatsapp:style/ConversationDate" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <View
            android:id="@*com.whatsapp:id/button_div"
            android:background="@*com.whatsapp:color/divider_gray"
            android:layout_width="fill_parent"
            android:layout_height="1.0dip" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <com.whatsapp.TextEmojiLabel
                android:textColor="@*com.whatsapp:drawable/flat_button_link_color_incoming"
                android:layout_gravity="center"
                android:id="@*com.whatsapp:id/view_contacts_btn"
                android:layout_width="0.0dip"
                android:layout_height="wrap_content"
                android:text="@*com.whatsapp:string/view_contacts_button"
                android:layout_weight="1.0"
                style="@*com.whatsapp:style/ConversationSharedContactButton" />
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:id="@*com.whatsapp:id/forward"
        style="@*com.whatsapp:style/ForwardButton" />
</merge>