<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:whatsapp="http://schemas.android.com/apk/com.whatsapp">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="262.0dip"
        android:layout_height="wrap_content"
        android:layout_marginRight="40.0dip">

        <FrameLayout
            android:id="@*com.whatsapp:id/quoted_message_holder"
            android:visibility="gone"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            style="@*com.whatsapp:style/QuotedFrame" />

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <FrameLayout
                android:layout_gravity="center|left"
                android:orientation="vertical"
                android:layout_width="0.0dip"
                android:layout_height="fill_parent"
                android:layout_weight="1.0">

                <LinearLayout
                    android:layout_gravity="left"
                    android:orientation="horizontal"
                    android:id="@*com.whatsapp:id/name_in_group"
                    android:paddingLeft="8.0dip"
                    android:paddingTop="3.0dip"
                    android:paddingRight="8.0dip"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <com.whatsapp.TextEmojiLabel
                        android:textSize="@*com.whatsapp:dimen/conversation_participant_name_tv"
                        android:textColor="#ffffffff"
                        android:id="@*com.whatsapp:id/name_in_group_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:lines="1"
                        android:singleLine="true" />

                    <com.whatsapp.TextEmojiLabel
                        android:textSize="@*com.whatsapp:dimen/conversation_participant_name_tv"
                        android:textColor="#ffdddddd"
                        android:id="@*com.whatsapp:id/pushname_in_group_tv"
                        android:paddingLeft="12.0sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:lines="1"
                        android:singleLine="true" />
                </LinearLayout>

                <LinearLayout
                    android:gravity="center"
                    android:layout_gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@*com.whatsapp:id/controls"
                    android:paddingTop="0.0dip"
                    android:paddingBottom="4.0dip"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">

                    <FrameLayout
                        android:gravity="center"
                        android:layout_width="54.0dip"
                        android:layout_height="54.0dip"
                        android:layout_marginLeft="4.0dip">

                        <ImageButton
                            android:gravity="center"
                            android:id="@*com.whatsapp:id/control_btn"
                            android:background="#00000000"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:src="@*com.whatsapp:drawable/inline_audio_play"
                            android:scaleType="center"
                            android:contentDescription="@*com.whatsapp:string/play" />

                        <com.whatsapp.CircularProgressBar
                            android:layout_gravity="center"
                            android:id="@*com.whatsapp:id/progress_bar_1"
                            android:visibility="gone"
                            android:layout_width="44.0dip"
                            android:layout_height="44.0dip"
                            android:indeterminateOnly="false" />
                    </FrameLayout>

                    <FrameLayout
                        android:layout_width="0.0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.0">

                        <com.whatsapp.VoiceNoteSeekBar
                            android:layout_gravity="center"
                            android:id="@*com.whatsapp:id/audio_seekbar"
                            android:paddingTop="8.0dip"
                            android:paddingBottom="8.0dip"
                            android:focusable="true"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            whatsapp:vnsbThumbSize="12.0dip"
                            whatsapp:vnsbTrackWidth="3.0dip" />
                    </FrameLayout>
                </LinearLayout>

                <TextView
                    android:textColor="#ffdddddd"
                    android:layout_gravity="bottom|center|left"
                    android:id="@*com.whatsapp:id/description"
                    android:paddingLeft="58.0dip"
                    android:paddingBottom="3.0dip"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    style="@*com.whatsapp:style/ConversationDate" />

                <LinearLayout
                    android:layout_gravity="bottom|center|right"
                    android:orientation="horizontal"
                    android:id="@*com.whatsapp:id/date_wrapper"
                    android:paddingRight="0.0dip"
                    android:paddingBottom="3.0dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="3.0dip">

                    <TextView
                        android:id="@*com.whatsapp:id/date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@*com.whatsapp:style/ConversationDate" />
                </LinearLayout>
            </FrameLayout>

            <FrameLayout
                android:id="@*com.whatsapp:id/thumbnail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <FrameLayout
                    android:layout_gravity="center"
                    android:id="@*com.whatsapp:id/picture_frame"
                    android:paddingLeft="10.0dip"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@*com.whatsapp:id/picture"
                        android:padding="4.0dip"
                        android:layout_width="62.0dip"
                        android:layout_height="62.0dip"
                        android:src="@*com.whatsapp:drawable/audio_message_thumb"
                        android:scaleType="fitCenter" />

                    <ImageView
                        android:layout_gravity="center"
                        android:id="@*com.whatsapp:id/picture_in_group"
                        android:padding="4.0dip"
                        android:layout_width="72.0dip"
                        android:layout_height="72.0dip"
                        android:src="@*com.whatsapp:drawable/audio_message_thumb"
                        android:scaleType="fitCenter" />

                    <ImageView
                        android:layout_gravity="center"
                        android:id="@*com.whatsapp:id/icon"
                        android:paddingBottom="12.0sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@*com.whatsapp:drawable/ic_audio"
                        android:scaleType="center" />

                    <FrameLayout
                        android:layout_gravity="center"
                        android:id="@*com.whatsapp:id/visualizer_frame"
                        android:paddingBottom="8.0dip"
                        android:layout_width="50.0dip"
                        android:layout_height="50.0dip" />

                    <TextView
                        android:textStyle="bold"
                        android:textColor="@*android:color/white"
                        android:layout_gravity="bottom|center"
                        android:id="@*com.whatsapp:id/duration"
                        android:paddingLeft="8.0dip"
                        android:paddingRight="8.0dip"
                        android:paddingBottom="6.0dip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@*com.whatsapp:style/ConversationDate" />
                </FrameLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:id="@*com.whatsapp:id/forward"
        style="@*com.whatsapp:style/ForwardButton" />
</merge>
