<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:gravity="top"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:id="@*com.google.android.apps.photos:id/photos_trash_ui_button_bar_buttons"
        android:background="@*com.google.android.apps.photos:color/quantum_white_100"
        android:paddingTop="6.0dip"
        android:paddingBottom="6.0dip"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <Button
            android:enabled="false"
            android:id="@*com.google.android.apps.photos:id/photos_trash_ui_button_bar_delete_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@*com.google.android.apps.photos:string/photos_trash_ui_action_mode_delete_button_label"
            android:layout_weight="1.0"
            style="@*com.google.android.apps.photos:style/ButtonBarButtonTextDisabled" />

        <Button
            android:enabled="false"
            android:id="@*com.google.android.apps.photos:id/photos_trash_ui_button_bar_restore_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@*com.google.android.apps.photos:string/photos_trash_ui_action_mode_restore_button_label"
            android:layout_weight="1.0"
            style="@*com.google.android.apps.photos:style/ButtonBarButtonTextDisabled" />
    </LinearLayout>

    <View
        android:id="@*com.google.android.apps.photos:id/trash_button_bar_fill_under_navigation_bar"
        android:background="@*android:color/background_material_dark"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip" />
</LinearLayout>