<?xml version="1.0" encoding="utf-8"?>
<FrameLayout android:background="@android:color/transparent"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <View
        android:layout_gravity="center_vertical"
        android:id="@*com.google.android.keep:id/background_view"
        android:background="@*android:color/background_material_dark"
        android:visibility="invisible"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip" />

    <LinearLayout
        android:layout_gravity="center_vertical"
        android:orientation="vertical"
        android:id="@*com.google.android.keep:id/share_handler_container"
        android:background="@*android:color/background_material_dark"
        android:visibility="visible"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:orientation="horizontal"
            android:id="@*com.google.android.keep:id/share_handler_header_web"
            android:visibility="gone"
            android:layout_width="fill_parent"
            android:layout_height="@*com.google.android.keep:dimen/share_dialog_web_title_height">

            <com.google.android.keep.share.TopCropImageView
                android:id="@*com.google.android.keep:id/web_image"
                android:background="@*com.google.android.keep:color/share_handler_default_background"
                android:layout_width="@*com.google.android.keep:dimen/share_dialog_web_title_height"
                android:layout_height="@*com.google.android.keep:dimen/share_dialog_web_title_height"
                android:src="@*com.google.android.keep:drawable/ic_language_black" />

            <LinearLayout
                android:layout_gravity="center_vertical"
                android:orientation="vertical"
                android:layout_width="0.0dip"
                android:layout_height="wrap_content"
                android:layout_weight="1.0">

                <TextView
                    android:id="@*com.google.android.keep:id/web_title"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    style="@*com.google.android.keep:style/ShareEditorWebTitle" />

                <TextView
                    android:id="@*com.google.android.keep:id/web_url"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    style="@*com.google.android.keep:style/ShareEditorWebSubTitle" />
            </LinearLayout>
        </LinearLayout>

        <EditText
            android:id="@*com.google.android.keep:id/share_handler_header"
            android:visibility="gone"
            android:layout_width="fill_parent"
            android:layout_height="@*com.google.android.keep:dimen/share_dialog_title_height"
            style="@*com.google.android.keep:style/ShareEditorTitle" />

        <View
            android:background="@*com.google.android.keep:color/share_handler_default_background"
            android:layout_width="fill_parent"
            android:layout_height="@*com.google.android.keep:dimen/divider_stroke_width" />

        <FrameLayout
            android:orientation="vertical"
            android:id="@*com.google.android.keep:id/share_handler_body_container"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:orientation="vertical"
                android:id="@*com.google.android.keep:id/share_handler_body"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <com.google.android.keep.share.MaxHeightScrollView
                    android:id="@*com.google.android.keep:id/dialog_scroll_view"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">

                        <include layout="@*com.google.android.keep:layout/share_images_layout" />

                        <EditText
                            android:id="@*com.google.android.keep:id/editable_text"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            style="@*com.google.android.keep:style/ShareEditorText" />
                    </LinearLayout>
                </com.google.android.keep.share.MaxHeightScrollView>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="@*com.google.android.keep:dimen/share_bottom_area_height">

                    <LinearLayout
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:id="@*com.google.android.keep:id/label_summary_image_container"
                        android:visibility="invisible"
                        android:layout_width="0.0dip"
                        android:layout_height="fill_parent"
                        android:layout_weight="1.0">

                        <ImageView
                            android:id="@*com.google.android.keep:id/label_icon"
                            android:paddingLeft="@*com.google.android.keep:dimen/share_account_side_margin"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@*com.google.android.keep:drawable/ic_label_gray"
                            android:contentDescription="@*com.google.android.keep:string/enter_label_name"
                            android:paddingStart="@*com.google.android.keep:dimen/share_account_side_margin" />

                        <FrameLayout
                            android:id="@*com.google.android.keep:id/label_summary_container"
                            android:layout_width="0.0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="1.0">

                            <com.google.android.keep.share.LabelSummaryLayout
                                android:gravity="center_vertical"
                                android:id="@*com.google.android.keep:id/label_summary"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                        </FrameLayout>
                    </LinearLayout>

                    <Button
                        android:textColor="@*com.google.android.keep:color/quantum_googblue"
                        android:gravity="center"
                        android:id="@*com.google.android.keep:id/save_button"
                        android:layout_width="wrap_content"
                        android:layout_height="fill_parent"
                        android:minWidth="@*com.google.android.keep:dimen/share_button_min_width"
                        android:text="@*com.google.android.keep:string/save"
                        style="@*com.google.android.keep:style/Widget.AppCompat.Button.Borderless" />
                </LinearLayout>

                <View
                    android:id="@*com.google.android.keep:id/account_selector_space"
                    android:visibility="invisible"
                    android:layout_width="fill_parent"
                    android:layout_height="@*com.google.android.keep:dimen/share_account_height" />
            </LinearLayout>

            <FrameLayout
                android:layout_gravity="bottom"
                android:id="@*com.google.android.keep:id/account_selector_container"
                android:background="@*com.google.android.keep:color/share_handler_account_selector"
                android:layout_width="fill_parent"
                android:layout_height="@*com.google.android.keep:dimen/share_account_height">

                <android.support.v7.widget.RecyclerView
                    android:id="@*com.google.android.keep:id/account_list"
                    android:visibility="invisible"
                    android:scrollbars="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent" />

                <include
                    android:id="@*com.google.android.keep:id/account_selector"
                    layout="@*com.google.android.keep:layout/share_handler_account" />
            </FrameLayout>

            <View
                android:id="@*com.google.android.keep:id/touch_interceptor"
                android:visibility="gone"
                android:clickable="true"
                android:layout_width="fill_parent"
                android:layout_height="0.0dip" />
        </FrameLayout>
    </LinearLayout>

    <include layout="@*com.google.android.keep:layout/share_label_selector" />
</FrameLayout>