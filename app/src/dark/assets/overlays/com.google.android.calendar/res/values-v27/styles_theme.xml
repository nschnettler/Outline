        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@*android:color/background_material_dark</item>
        <item name="android:navigationBarDividerColor">#1a000000</item>
        <item name="android:windowLightStatusBar">false</item>
